{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/robots.txt",
			"destination": "public/robots.txt"
		},
		{
			"source": "/",
			"destination": "public/index.html"
		},
		{
			"source": "/favicon.ico",
			"destination": "public/favicon.ico"
		},
		{
			"source": "/favicon.png",
			"destination": "public/android-chrome-512x512.png"
		},
		{
			"source": "/(.*)",
			"destination": "api/handler"
		}
	]
}