{
	"compilerOptions": {
		"resolveJsonModule": true,
		"lib": ["ESNext", "DOM", "dom.iterable"],
		"moduleResolution": "node",
		"module": "ESNext",
		"target": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	}
}
